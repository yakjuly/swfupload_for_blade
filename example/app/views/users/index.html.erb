<%= link_to "new", new_user_path %>
<table>
  <tr>
    <th>name</th>
    <th>Attachment</th>
    <th>oper</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.name, edit_user_path(user) %></td>
      <td><%= link_to_attachment(user.attachment) %></td>
      <td><%= link_to "destroy", user_path(user), :method => :delete %></td>
    </tr>
  <% end %>
</table>